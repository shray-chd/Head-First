import java.util.*;

public class developer {
	
	int age=30;
	String firstName="Shray";
	String lastName="Chadha";
	public ArrayList<Tools> toolList = new ArrayList<Tools>();
	
	public void setUpTools(){
		
		Tools t1 = new Tools();
		t1.setToolName("Java");
		t1.expInTool(3);
		
		Tools t2 = new Tools();
		t1.setToolName("CSS");
		t1.expInTool(3);
		
		Tools t3 = new Tools();
		t1.setToolName("SQL");
		t1.expInTool(2);
		
		toolList.add(t1);
		toolList.add(t2);
		toolList.add(t3);			
		
	}
	
	public void printDevTools(){
		
		for(Tools cell : toolList){
			System.out.println(cell.nameOfTool);
			System.out.println(cell.noOfYears);
		}
		
	}
	
	
}
	
	
	
